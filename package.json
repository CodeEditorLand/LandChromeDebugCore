{
  "name": "vscode-chrome-debug-core",
  "displayName": "vscode-chrome-debug-core",
  "version": "6.8.11",
  "description": "A library for building VS Code debug adapters for targets that support the Chrome Remote Debug Protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-chrome-debug-core"
  },
  "bugs": "https://github.com/microsoft/vscode-chrome-debug-core/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "typings": "./lib/src/index.d.ts",
  "main": "./out/src/index",
  "dependencies": {
    "@types/source-map": "^0.5.2",
    "color": "^4.2.3",
    "devtools-protocol": "0.0.1315554",
    "glob": "^10.4.1",
    "noice-json-rpc": "^1.2.0",
    "source-map": "^0.7.4",
    "vscode-debugadapter": "^1.47.0",
    "vscode-debugprotocol": "^1.37.0",
    "vscode-nls": "^5.2.0",
    "vscode-uri": "^3.0.8",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/glob": "^8.1.0",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.6",
    "@types/mockery": "^1.4.33",
    "@types/node": "^20.14.5",
    "@types/ws": "^8.5.10",
    "del": "^7.1.0",
    "event-stream": "^4.0.1",
    "gulp": "^5.0.0",
    "gulp-debug": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "gulp-util": "^3.0.7",
    "merge2": "^1.4.1",
    "minimist": "^1.2.8",
    "mocha": "^10.4.0",
    "mockery": "^2.1.0",
    "nyc": "^17.0.0",
    "source-map-support": "^0.5.21",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typemoq": "^2.1.0",
    "typescript": "^5.4.5",
    "vscode-nls-dev": "^4.0.4"
  },
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "mocha --exit --recursive -u tdd ./out/test/",
    "testCoverage": "nyc mocha --require source-map-support/register  --exit --recursive -u tdd ./out/test/**/*.test.js",
    "prepare": "gulp build",
    "tslint": "gulp tslint",
    "patch": "npm version patch -m '%s'",
    "minor": "npm version minor -m '%s'",
    "major": "npm version major -m '%s'"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/coverage/**/*.*",
      "**/testData/**/*.*"
    ],
    "reporter": "html",
    "all": true
  }
}
