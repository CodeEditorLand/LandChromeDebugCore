{
	"name": "vscode-chrome-debug-core",
	"displayName": "vscode-chrome-debug-core",
	"version": "0.0.1",
	"private": false,
	"description": "A library for building VS Code debug adapters for targets that support the Chrome Remote Debug Protocol",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandChromeDebugCore#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandChromeDebugCore/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandChromeDebugCore.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/src/index",
	"typings": "./lib/src/index.d.ts",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "gulp build",
		"major": "npm version major -m '%s'",
		"minor": "npm version minor -m '%s'",
		"patch": "npm version patch -m '%s'",
		"prepare": "gulp build",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"testCoverage": "nyc mocha --require source-map-support/register  --exit --recursive -u tdd ./out/test/**/*.test.js"
	},
	"nyc": {
		"all": true,
		"exclude": [
			"**/*.d.ts",
			"**/coverage/**/*.*",
			"**/testData/**/*.*"
		],
		"extension": [
			".ts"
		],
		"reporter": "html"
	},
	"dependencies": {
		"@types/source-map": "^0.1.27",
		"color": "^3.1.3",
		"devtools-protocol": "0.0.588169",
		"glob": "^7.1.3",
		"noice-json-rpc": "^1.2.0",
		"source-map": "^0.6.1",
		"ws": "^6.0.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/color": "^3.0.0",
		"@types/glob": "^5.0.35",
		"@types/minimatch": "^2.0.29",
		"@types/mockery": "^1.4.29",
		"@types/node": "^8.0.58",
		"@types/ws": "^6.0.0",
		"del": "^2.2.2",
		"event-stream": "^3.3.4",
		"merge2": "^1.0.2",
		"minimist": "^1.2.5",
		"mockery": "^1.7.0",
		"nyc": "^14.1.1",
		"source-map-support": "^0.5.12",
		"typemoq": "^2.1.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
